<%- include('partials/header') %> 
<%- include('partials/navbar') %> 

<style>
    body{
        background: linear-gradient(to right,#e9a9c7, #c55682, #dc6895);
    }
    #texto{
        text-align: center ;
        font-size: larger;
    }
</style>

<div class="container" id="texto">

    <% produtos.forEach(produto => {%>
    <table>
        <tr>
            <td>
                <p><%= produto.titulo %></p>
            </td>
            <td>
                <p> : <%= produto.descricao %></p>
            </td>
        </tr>
    </table>
    <% }) %>
    
        
        
     

</div>


